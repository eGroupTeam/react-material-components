    "js-base64": "^3.3.3",
    "mobile-detect": "^1.4.4",
    "redux-actions": "^2.6.5",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.5.3",
    "validator": "^13.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@types/lodash.clonedeep": "^4.5.6",
