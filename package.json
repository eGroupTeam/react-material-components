  "scripts": {
    "start": "start-storybook -s ./public -p 9001 --https --ssl-cert ./certificate/certificate.crt --ssl-key ./certificate/privateKey.key",
    "build-storybook": "build-storybook -s public",
    "build:tsd": "tsc -p ./tsconfig.build.json",
    "test": "jest",
    "test:dev": "jest --watch",
    "build": "lerna run build && yarn build:tsd && lerna run build:copy-files",
    "release": "lerna version && yarn build && lerna publish from-git --contents build",
    "release:force": "lerna version --force-publish && yarn build && lerna publish from-git --contents build --force-publish",
    "lint": "eslint --ext .jsx,.js,.ts,.tsx ./ --fix",
    "generate-changelog": "github_changelog_generator -u eGroupAI -p egroup-material"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "packages/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "stories/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "workspaces": [
    "packages/*"
  ]
